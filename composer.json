{
    "name": "levizoesch/laravelsetenvironment",
    "type": "project",
    "description": "A simple command to set environment variables via command line. I wrote this to assist in pipeline builds.",
    "keywords": [
        "Levi Zoesch",
        "laravel",
        "laravel commands",
        "laravel helpers",
        "Laravel Set Environment"
    ],
  "require": {
        "php": "^8.0",
        "composer/installers": "~1.0"
  },
    "scripts": {
        "post-install-cmd": [
            "cp src/SetEnvironmentVariable.php /App/Console/Commands/"
        ],
        "post-update-cmd": [
            "cp src/SetEnvironmentVariable.php /App/Console/Commands/"
        ]
    }


}
  
