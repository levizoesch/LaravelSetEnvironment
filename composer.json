{
    "name": "levizoesch/laravel-set-environment",
    "type": "project",
    "license": "MIT",
    "description": "A simple command to set environment variables via command line. I wrote this to assist in pipeline builds.",
    "keywords": [
        "Levi Zoesch",
        "laravel",
        "laravel commands",
        "laravel helpers",
        "Laravel Set Environment"
    ],
    "require": {
        "php": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4" : {
            "levizoesch\\laravelsetenvironment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "levizoesch\\laravelsetenvironment\\SetEnvironmentVariableServiceProvider"
            ]
        }
    }
}
